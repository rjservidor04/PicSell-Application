<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:ns1="http://schemas.android.com/tools">
    <string-array name="profile_options">
        <item>View User Profile</item>
        <item>Change Password</item>
        <item>Change Profile Details</item>
        <item>Sign Out</item>
    </string-array>
    <color name="black">#FF000000</color>
    <color name="blue_100">#FF3F51B5</color>
    <color name="orange_100">#FFFF9913</color>
    <color name="orange_200">#FFE68910</color>
    <color name="orange_300">#FFFCD299</color>
    <color name="purple_200">#FFBB86FC</color>
    <color name="purple_500">#FF6200EE</color>
    <color name="purple_700">#FF3700B3</color>
    <color name="teal_200">#FF03DAC5</color>
    <color name="teal_700">#FF018786</color>
    <color name="white">#FFFFFFFF</color>
    <dimen name="fab_margin">16dp</dimen>
    <string name="action_settings">Action Settings</string>
    <string name="app_name">PicSell</string>
    <style name="Theme.PicSellApplication" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        
        <item name="colorPrimary">@color/orange_100</item>
        <item name="colorPrimaryVariant">@color/orange_200</item>
        <item name="colorOnPrimary">@color/white</item>
        
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        
        <item name="android:statusBarColor" ns1:targetApi="l">?attr/colorPrimaryVariant</item>
        
    </style>
    <style name="Theme.PicSellApplication.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar"/>
    <style name="Theme.PicSellApplication.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>
    <style name="Theme.PicSellApplication.PopupOverlay" parent="ThemeOverlay.AppCompat.Light"/>
